{
  "name": "tree-sitter-canapea",
  "version": "0.0.24",
  "description": "Canapea grammar for tree-sitter",
  "repository": "https://github.com/canapea/canapea",
  "license": "UPL-1.0",
  "author": {
    "name": "Martin Feineis",
    "email": "mfeineis@users.noreply.github.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "canapea"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "//": "Had to make it non-optional, .npmrc include=optional wouldn't cut it",
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": false
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter generate --abi 14 --build && tree-sitter build --wasm",
    "start": "tree-sitter playground --quiet",
    "pretest": "tree-sitter generate --abi 14 --build",
    "test": "node --test bindings/node/*_test.js"
  }
}
