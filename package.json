{
  "name": "tree-sitter-lang0",
  "version": "0.0.1",
  "description": "Lang0 grammar for tree-sitter",
  "repository": "https://github.com/mfeineis/tree-sitter-lang0",
  "license": "UPL",
  "author": {
    "name": "Martin Feineis",
    "email": "mfeineis@users.noreply.github.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "engines": {
    "node": ">=22.14.0"
  },
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "lang0"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
