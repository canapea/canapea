module "canapea/experimental/lang/tuple"
  exposing
    | Tuple
    | new


# TODO: Not sure we actually want tuples, there's no
#       special syntax planned for them and records
#       are pretty much always better since our
#       functions aren't auto-curried. If you really
#       want them you can easily define them for yourself

type Tuple a b =
  | Tuple a b


let new x y =
  Tuple x y

